{
  "name": "spelledright",
  "version": "1.0.0",
  "description": "This project is a js library used to facilitate spell checks on HTML pages",
  "main": "index.js",
  "scripts": {
    "unittest": "jasmine spec/*unit.spec.js",
    "perftest": "jasmine spec/*perf.spec.js",
    "coverage": "istanbul cover jasmine && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "grunt babel && npm run-script test && npm run-script coverage"
  },
  "keywords": [
    "js",
    "spellcheck",
    "spell-check",
    "spelling",
    "testing",
    "checker",
    "spelling",
    "spell"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "jasmine": "^2.4.1",
    "typo-js": "^1.0.1",
    "xmldom": "^0.1.22"
  },
  "devDependencies": {
    "coveralls": "^2.11.8",
    "eslint": "^2.3.0",
    "eslint-config-google": "^0.4.0",
    "express": "^4.13.4",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-jasmine": "^1.0.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-eslint": "^18.0.0",
    "istanbul": "^0.4.2",
    "request": "^2.69.0",
    "selenium-webdriver": "^2.52.0"
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "jasmine": true,
      "node": true,
      "es6": true
    }
  }
}
